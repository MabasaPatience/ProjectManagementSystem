@model IEnumerable<Project_Management_System.Models.Project_Team>

@{
    ViewBag.Title = "ReportTeam";
}




<table class="table table-striped  table-hover ">
    <thead class="tf">
        <tr>
            <th>
                @Html.DisplayName("ID")
            </th>
            <th>
                @Html.DisplayName("Email")
            </th>
            <th>
                @Html.DisplayName("Role")
            </th>
            <th>
                @Html.DisplayName("Responsebility")
            </th>


        </tr>
        </thead>

        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.T_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.T_Email)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.T_Role)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.T_Responsebility)
                </td>


            </tr>
        }

</table>
<p>
    @Html.ActionLink("Back", "Report", new { id = ViewBag.ProjectID }, new { @class = "btn btn-info" }) |
    @Html.ActionLink("Next", "ReportStakeholders", new { id = ViewBag.ProjectID },  new { @class = "btn btn-info" })
</p>

@model IEnumerable<Project_Management_System.Models.Cost_Estimation>

@{
    ViewBag.Title = "ReportResources";
}

<h4 class="bg-warning">ReportResources</h4>


<table class="table table-striped  table-hover ">
    <thead class="tf">
        <tr>
            <th>
                @Html.DisplayName("ID")
            </th>
            <th>
                @Html.DisplayName("name")
            </th>
            <th>
                @Html.DisplayName("cost")
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Total_Cost)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.P_ID)
            </th>
            <th></th>
        </tr>
        </thead>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.R_ID)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.R_name)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.R_cost)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.Total_Cost)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.P_ID)
                </td>

            </tr>
        }

</table>
<p>
    @Html.ActionLink("Back", "ReportStakeholders", new { id = ViewBag.ProjectID }, new { @class = "btn red" }) |
    @Html.ActionLink("Next",  "Index" , "DisplayProject", new { id = ViewBag.ProjectID }, new { @class = "btn btn-info" }) |

</p>